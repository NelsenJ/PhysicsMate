<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<VBox alignment="CENTER" spacing="15" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="physicsmate.controller.MenuController">
    <padding>
        <Insets top="30" bottom="30" left="50" right="50"/>
    </padding>

    <Button text="Energi Kinetik (Ek = ½ m v²)" onAction="#openKineticEnergy" minWidth="220"/>
    <Button text="Hukum Newton II (F = m × a)" onAction="#openNewtonSecondLaw" minWidth="220"/>
    <Button text="Gelombang (v = f × λ)" onAction="#openWave" minWidth="220"/>

</VBox>
